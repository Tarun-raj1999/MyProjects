package minAndMax;

public class MaxNumber {

	public void max()
	{
		int []arr= {20,10,30,25};
		for(int i=0;i<arr.length;i++)
		{
			for(int j=1;j<arr.length;j++)
			{
			   
			    		if(arr[i]>arr[j])
			    		{
			    			System.out.println(arr[i]);
			    		}
			}
		}
	}
	public static void main(String[] args) {
	
		MaxNumber m=new MaxNumber();
		m.max();

	}

}
