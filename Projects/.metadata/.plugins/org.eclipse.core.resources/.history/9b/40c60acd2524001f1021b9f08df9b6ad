package tarun.basic;

public class Multidimentional {

	public static void main(String[] args) {
		int size=2;
		int a[][]=new int[size][size];
		a[0][0]=10;
		a[0][1]=20;
		a[1][0]=30;
		a[1][1]=40;
		for(int i=0;i<a.length;i++)
		{
			for(int j=0;j<a.length;j++)
			{
				System.out.println(a[i][j]);
			}
			System.out.println("");
		}
		

	}

}
