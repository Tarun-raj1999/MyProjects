package tarun.basic;

public class Pattern4 {

	public static void main(String[] args) {
	int n=5;
		for(int row=0;row<n;row++)
		{
			for(int space=0;space<row;space++)
			{
			System.out.print(" ");
		}
			for(int col=7;col>row;col--)
			{
				System.out.print(col);
			}
			System.out.println("");

	}

 }
}