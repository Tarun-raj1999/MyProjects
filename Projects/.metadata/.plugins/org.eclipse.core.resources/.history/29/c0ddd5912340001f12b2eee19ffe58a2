package minAndMax;

public class MaxNumber {

	public void max()
	{
		int arr[]= new int[]{12,14,50,2};
		
		int max=arr[0];
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]>max)//
			{
			max=arr[i];
			}
			
		}
		System.out.println(max);
	}
	public static void main(String[] args) {
	
		MaxNumber m=new MaxNumber();
		m.max();

	}

}
