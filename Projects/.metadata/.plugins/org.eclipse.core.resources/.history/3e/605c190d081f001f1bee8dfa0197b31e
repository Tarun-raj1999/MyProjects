package tarun.basic;

public class Pattern4 {

	public static void main(String[] args) {
	int n=5;
	int col=7;
		for(int row=0;row<n;row++)
		{
			for(int space=0;space<row;space++)
			{
			System.out.print(" ");
		}
			for(int co=col-row;co<=n+2;co--)
			{
				System.out.print(co);
			}
			System.out.println("");

	}

 }
}